<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0-windows10.0.17763.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>latest</LangVersion>
    <PublishSingleFile>true</PublishSingleFile>
    <PublishRelease>true</PublishRelease>
    <SelfContained>false</SelfContained>
    <IsPackable>false</IsPackable>
    <Company>Katrea.Net</Company>
    <Product>WinStoreLinks</Product>
    <AssemblyName>WinStoreLinks</AssemblyName>
    <AssemblyTitle>WinStoreLinks</AssemblyTitle>
    <AssemblyVersion>2023.10.1</AssemblyVersion>
    <RootNamespace>Net.Katrea.WinStoreLinks</RootNamespace>
    <!--<ApplicationIcon>browser.ico</ApplicationIcon>-->

  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Ookii.CommandLine" Version="4.0.1" />
  </ItemGroup>

</Project>
